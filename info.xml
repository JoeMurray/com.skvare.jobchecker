<?xml version="1.0"?>
<extension key="com.skvare.jobchecker" type="module">
  <file>jobchecker</file>
  <name>Scheduled Job Checker</name>
  <description>Analyse Scheduled Job and alert to admin on failure detection</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Sunil Pawar</author>
    <email>sunil@skvare.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/Skvare/com.skvare.jobchecker</url>
    <url desc="Documentation">https://github.com/Skvare/com.skvare.jobchecker</url>
    <url desc="Support">https://github.com/Skvare/com.skvare.jobchecker</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-08-30</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Jobchecker</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
